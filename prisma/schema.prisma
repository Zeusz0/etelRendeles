generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model felhasznalo{
  id Int @id @default(autoincrement())
  vezetek_nev String
  kereszt_nev String
  telefonszam String
  cim String
  szamlazasi_cim String
  email String
  partner Boolean @default(false)
  admin Boolean @default(false)
}


model menu {
  id    Int     @id @default(autoincrement())
  kategoriak String
}

model aru {
  id    Int     @id @default(autoincrement())
  nev String
  leiras  String
  ar  Int
  kep String
}

model rendeles {
  id    Int     @id @default(autoincrement())
  termekek String
  kisszalitasi_dij  Int
  borravalo  Int
  aruosszeg  Int
  osszeg  Int
}
